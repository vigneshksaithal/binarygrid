    "type-check": "tsc --build",
    "test": "vitest run",
    "local": "vite dev --port 7474"
  },
  "dependencies": {
    "@devvit/start": "^0.12.13",
    "@devvit/test": "^0.12.13",
    "@devvit/web": "0.12.13",
    "@hono/node-server": "^1.19.9",
    "@lucide/svelte": "^0.575.0",
    "@tailwindcss/vite": "^4.2.1",
    "canvas-confetti": "^1.9.4",
    "devvit": "0.12.13",
    "hono": "^4.11.10",
    "tailwindcss": "^4.2.1"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^7.0.0",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^25.3.1",
    "concurrently": "^9.2.1",
    "dotenv-cli": "^11.0.0",
    "fast-check": "^4.5.3",
    "svelte": "^5.53.5",
    "svelte-check": "^4.4.3",
    "typescript": "^5.9.3",
    "vite": "7.3.1",
    "vitest": "^4.0.18"
