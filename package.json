{
  "private": true,
  "name": "binarygrid",
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "postinstall": "npm run build",
    "build": "vite build",
    "deploy": "npm run build && devvit upload",
    "dev": "devvit playtest",
    "preview": "vite preview",
    "login": "devvit login",
    "check": "svelte-check --tsconfig ./src/client/tsconfig.json",
